{
  "name": "sf-container-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": "./build/index.js",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "check-ts": "tsc --noEmit --skipLibCheck",
    "test": "node --import=tsx --test --experimental-test-module-mocks --test-concurrency=2 \"./src/**/*.test.ts\"",
    "test:src": "cd ./src && node --experimental-test-module-mocks --import=tsx --test --test-concurrency=2",
    "test:pattern": "node --import=tsx --test --experimental-test-module-mocks --test-concurrency=2"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@sinclair/typebox": "^0.34.38",
    "ajv": "^8.17.1",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "snowflake-sdk": "^2.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
